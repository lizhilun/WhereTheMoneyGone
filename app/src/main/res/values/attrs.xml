<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTitleBar">
        <attr name="backBtnVisible" format="boolean" />
        <attr name="titleText" format="string|reference" />
        <attr name="actionText" format="string|reference" />
        <attr name="backBtnSrc" format="reference" />
    </declare-styleable>

    <declare-styleable name="SkinImageView">
        <attr name="android:src" />
        <attr name="android:tint" />
        <attr name="srcCompat" />
    </declare-styleable>

    <declare-styleable name="InputItemEditView">
        <attr name="decText" format="string|reference" />
        <attr name="hint" format="string|reference" />
        <attr name="hintTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TextViewWithDes">
        <attr name="decTextStr" format="string|reference" />
        <attr name="decTextAppearance" format="reference" />
        <attr name="mainTextAppearance" format="reference" />
        <attr name="rightArrowVisible" format="boolean" />
        <attr name="viewType" format="enum">
            <enum name="top_bottom" value="1" />
            <enum name="start_end" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="QuantityStatisticsView">
        <attr name="title" format="string|reference" />
    </declare-styleable>

</resources>